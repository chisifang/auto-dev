<idea-plugin package="cc.unitmesh.csharp">
    <!--suppress PluginXmlValidity -->
    <dependencies>
        <plugin id="com.intellij.modules.rider"/>
    </dependencies>

    <extensions defaultExtensionNs="cc.unitmesh">
        <classContextBuilderNew language="C#"
                             implementationClass="cc.unitmesh.csharp.context.CsharpClassContextBuilder"/>

        <contextPrompterNew language="C#" implementation="cc.unitmesh.csharp.provider.CsharpContextPrompter"/>
    </extensions>
</idea-plugin>
