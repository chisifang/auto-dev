<idea-plugin package="cc.unitmesh.rust">
    <!--suppress PluginXmlValidity -->
    <dependencies>
        <plugin id="com.intellij.clion"/>
        <plugin id="com.intellij.cidr.base"/>

        <module name="org.rust.lang"/>
    </dependencies>

    <extensions defaultExtensionNs="cc.unitmesh">
        <classContextBuilderNew language="Rust"
                             implementationClass="cc.unitmesh.rust.context.RustClassContextBuilder"/>
        <methodContextBuilderNew language="Rust"
                              implementationClass="cc.unitmesh.rust.context.RustMethodContextBuilder"/>

        <contextPrompterNew language="Rust" implementation="cc.unitmesh.rust.provider.RustContextPrompter"/>

        <chatContextProviderNew implementation="cc.unitmesh.rust.provider.RustContextProvider"/>
    </extensions>
</idea-plugin>
